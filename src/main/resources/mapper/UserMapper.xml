<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.server.mapper.UserMapper">

    <resultMap id="BaseResultMap" type="com.server.pojo.User">
            <id property="userid" column="UserId" jdbcType="INTEGER"/>
            <result property="username" column="UserName" jdbcType="VARCHAR"/>
            <result property="gender" column="gender" jdbcType="INTEGER"/>
            <result property="permission" column="permission" jdbcType="INTEGER"/>
            <result property="faceinfo" column="faceInfo" jdbcType="VARCHAR"/>
    </resultMap>

    <sql id="Base_Column_List">
        UserId,UserName,gender,
        permission,faceInfo
    </sql>
</mapper>
